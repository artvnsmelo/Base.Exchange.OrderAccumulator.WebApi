{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MongoDbSettings": {
    "ConnectionString": "mongodb://localhost:27017",
    "DatabaseName": "base_exchange"
  },
  "HostConfig": {
    "ResourcesPath": "Resources",
    "CulturesNames": [
      "pt-BR",
      "en-US"
    ]
  },
  "SwaggerConfig": {
    "Enabled": true,
    "Title": "Base.Exchange.OrderAccumulator.WebApi"
  },
  "HttpManager": {
    "WaitAndRetry": {
      "SleepSecondsDurations": [
        1,
        2,
        3
      ]
    }
  }
}
